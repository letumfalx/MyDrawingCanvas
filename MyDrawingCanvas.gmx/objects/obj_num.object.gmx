var count = string_count(".", global.ip_address);
var temp_count = 0;
var strlen = string_length(global.ip_address);
var end_number = false;   


if(code == "&gt;") {
    if(count == 3 &amp;&amp; string_char_at(global.ip_address, strlen) != ".") {
        ini_open("network");
        ini_write_string("client", "lastIP", global.ip_address);
        ini_close();
        //show_message("OK IP ADDRESS");
        room_goto(rm_canvas);
    }
} else if(code == "&lt;") {
    if(strlen &gt; 0) {
        if(string_char_at(global.ip_address, strlen) == ".") {
            global.ip_address = string_copy(global.ip_address, 1, strlen - 2);
        } else {
            global.ip_address = string_copy(global.ip_address, 1, strlen - 1);            
        }
    }
} else if(code == ".") {
    if(strlen &gt; 0 &amp;&amp; string_char_at(global.ip_address, strlen) != "." 
            &amp;&amp; count &lt; 3) {
        global.ip_address += ".";
    }
} else if(code == "C") { 
    global.ip_address = "";
}
else {
    var last_point = 0;
    for(var ind = 1; ind &lt;= strlen; ind++) {
        if(string_char_at(global.ip_address, ind) == "."){
            last_point = ind;
            temp_count++;
            if(temp_count == count) break;
        }
    }
    
    var len = strlen - last_point;
    if(count &gt;= 3) {
        if(len &lt;= 2) {
            global.ip_address += code;
        }
    } else {
        global.ip_address += code;
        if(len &gt; 1) {
            global.ip_address += ".";
        }
    }
    
}



</string>
